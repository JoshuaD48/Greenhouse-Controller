Returns:\par}\pard\plain \s62\li720\widctlpar\ql\adjustright \fs20\cgrid ht221sData_s temperature and humidity data \par
}}{
\pard\plain \s41\li360\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid   286 \{\par
  287     ht221sData_s rd = \{0\};\par
  288 #if EMULATOR\par
  289     PyRun_SimpleString(\par
  290         "from sense_emu import SenseHat\\n"\par
  291         "#from time import time,ctime\\n"\par
  292         "#print('Today is '+ctime(time))\\n"\par
  293         "sense=SenseHat()\\n"\par
  294         "temp=sense.temp\\n"\par
  295         "humid=sense.humidity\\n"\par
  296         "#print(temp)\\n"\par
  297         "#print(humid)\\n"\par
  298         "f=open(\\"tempfileforpython.txt\\",\\"w\\")\\n"\par
  299         "f.write(repr(temp))\\n"\par
  300         "f.close()\\n"\par
  301         "f=open(\\"humifileforpython.txt\\",\\"w\\")\\n"\par
  302         "f.write(repr(humid))\\n"\par
  303         "f.close()\\n"\par
  304         );\par
  305     double reading=0;\par
  306     FILE *fp;\par
  307     fp=fopen("tempfileforpython.txt","r");\par
  308     fscanf(fp, "%lf", &reading);\par
  309     fclose(fp);\par
  310     rd.temperature = reading;\par
  311     //fprintf(stdout, "%lf\\n", reading);\par
  312     fp=fopen("humifileforpython.txt","r");\par
  313     fscanf(fp, "%lf", &reading);\par
  314     fclose(fp);\par
  315     //fprintf(stdout, "%lf\\n", reading);\par
  316     rd.humidity = reading;\par
  317 #else\par
  318     int status;\par
  319     uint8_t t0_out_l,t0_out_h,t1_out_l,t1_out_h;\par
  320     uint8_t t0_degC_x8,t1_degC_x8,t1_t0_msb;\par
  321     int16_t T0_OUT,T1_OUT;\par
  322     uint16_t T0_DegC_x8,T1_DegC_x8;\par
  323     double T0_DegC,T1_DegC;\par
  324     double t_gradient_m,t_intercept_c;\par
  325     uint8_t t_out_l,t_out_h;\par
  326     int16_t T_OUT;\par
  327     uint8_t h0_out_l,h0_out_h,h1_out_l,h1_out_h,h0_rh_x2,h1_rh_x2,h_t_out_l,h_t_out_h;\par
  328     int16_t H0_T0_OUT,H1_T0_OUT,H_T_OUT;\par
  329     double H0_rH,H1_rH,h_gradient_m,h_intercept_c;\par
  330 \par
  331     // Power down the device (clean start)\par
  332     wiringPiI2CWriteReg8(HTS221fd, CTRL_REG1, 0x00);\par
  333     // Turn on the humidity sensor analog front end in single shot mode\par
  334     wiringPiI2CWriteReg8(HTS221fd, CTRL_REG1, 0x84);\par
  335     // Run one-shot measurement (temperature and humidity). The set bit will be reset by the\par
  336     // sensor itself after execution (self-clearing bit)\par
  337     wiringPiI2CWriteReg8(HTS221fd, CTRL_REG2, 0x01);\par
  338 \par
  339     // Wait until the measurement is completed\par
  340     do\par
  341     \{\par
  342         usleep(HTS221DELAY);    // 25 ms\par
  343         status = wiringPiI2CReadReg8(HTS221fd, CTRL_REG2);\par
  344     \}\par
  345     while (status != 0);\par
  346 \par
  347     // Read calibration temperature LSB (ADC) data\par
  348     // (temperature calibration x-data for two points)\par
  349     t0_out_l = wiringPiI2CReadReg8(HTS221fd, T0_OUT_L);\par
  350     t0_out_h = wiringPiI2CReadReg8(HTS221fd, T0_OUT_H);\par
  351     t1_out_l = wiringPiI2CReadReg8(HTS221fd, T1_OUT_L);\par
  352     t1_out_h = wiringPiI2CReadReg8(HTS221fd, T1_OUT_H);\par
  353 \par
  354    // Read calibration relative humidity LSB (ADC) data\par
  355     // (humidity calibration x-data for two points)\par
  356     h0_out_l = wiringPiI2CReadReg8(HTS221fd, H0_T0_OUT_L);\par
  357     h0_out_h = wiringPiI2CReadReg8(HTS221fd, H0_T0_OUT_H);\par
  358     h1_out_l = wiringPiI2CReadReg8(HTS221fd, H1_T0_OUT_L);\par
  359     h1_out_h = wiringPiI2CReadReg8(HTS221fd, H1_T0_OUT_H);\par
  360 \par
  361     // Read calibration temperature (\'B0C) data\par
  362     // (temperature calibration y-data for two points)\par
  363     t0_degC_x8 = wiringPiI2CReadReg8(HTS221fd, T0_degC_x8);\par
  364     t1_degC_x8 = wiringPiI2CReadReg8(HTS221fd, T1_degC_x8);\par
  365     t1_t0_msb = wiringPiI2CReadReg8(HTS221fd, T1_T0_MSB);\par
  366 \par
  367    // Read relative humidity (% rH) data\par
  368     // (humidity calibration y-data for two points)\par
  369     h0_rh_x2 = wiringPiI2CReadReg8(HTS221fd, H0_rH_x2);\par
  370     h1_rh_x2 = wiringPiI2CReadReg8(HTS221fd, H1_rH_x2);\par
  371 \par
  372     // make 16 bit values (bit shift)\par
  373     // (temperature calibration x-values)\par
  374     T0_OUT = t0_out_h << 8 | t0_out_l;\par
  375     T1_OUT = t1_out_h << 8 | t1_out_l;\par
  376 \par
  377     // make 16 and 10 bit values (bit mask and bit shift)\par
  378     T0_DegC_x8 = (t1_t0_msb & 3) << 8 | t0_degC_x8;\par
  379     T1_DegC_x8 = ((t1_t0_msb & 12) >> 2) << 8 | t1_degC_x8;\par
  380 \par
  381     // Calculate calibration values\par
  382     // (temperature calibration y-values)\par
  383     T0_DegC = T0_DegC_x8 / 8.0;\par
  384     T1_DegC = T1_DegC_x8 / 8.0;\par
  385 \par
  386     // Solve the linear equasions 'y = mx + c' to give the\par
  387     // calibration straight line graphs for temperature and humidity\par
  388     t_gradient_m = (T1_DegC - T0_DegC) / (T1_OUT - T0_OUT);\par
  389     t_intercept_c = T1_DegC - (t_gradient_m * T1_OUT);\par
  390 \par
  391     // Read the ambient temperature measurement (2 bytes to read)\par
  392     t_out_l = wiringPiI2CReadReg8(HTS221fd, TEMP_OUT_L);\par
  393     t_out_h = wiringPiI2CReadReg8(HTS221fd, TEMP_OUT_H);\par
  394 \par
  395     // make 16 bit value\par
  396     T_OUT = t_out_h << 8 | t_out_l;\par
  397 \par
  398     // make 16 bit values (bit shift)\par
  399     // (humidity calibration x-values)\par
  400     H0_T0_OUT = h0_out_h << 8 | h0_out_l;\par
  401     H1_T0_OUT = h1_out_h << 8 | h1_out_l;\par
  402 \par
  403     // Humidity calibration values\par
  404     // (humidity calibration y-values)\par
  405     H0_rH = h0_rh_x2 / 2.0;\par
  406     H1_rH = h1_rh_x2 / 2.0;\par
  407     h_gradient_m = (H1_rH - H0_rH) / (H1_T0_OUT - H0_T0_OUT);\par
  408     h_intercept_c = H1_rH - (h_gradient_m * H1_T0_OUT);\par
  409 \par
  410     // Read the ambient humidity measurement (2 bytes to read)\par
  411     h_t_out_l = wiringPiI2CReadReg8(HTS221fd, H_T_OUT_L);\par
  412     h_t_out_h = wiringPiI2CReadReg8(HTS221fd, H_T_OUT_H);\par
  413 \par
  414     // make 16 bit value\par
  415     H_T_OUT = h_t_out_h << 8 | h_t_out_l;\par
  416 \par
  417     // Power down the device\par
  418     wiringPiI2CWriteReg8(HTS221fd, CTRL_REG1, 0x00);\par
  419 \par
  420     // Calculate and return ambient temperature\par
  421     rd.temperature = (t_gradient_m * T_OUT) + t_intercept_c;\par
  422     rd.humidity = (h_gradient_m * H_T_OUT) + h_intercept_c;\par
  423 #endif\par
  424     return rd;\par
  425 \}\par
}
}
