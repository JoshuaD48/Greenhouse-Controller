\pard\plain \s41\li360\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid   210 \{\par
  211     lps25hData_s rd = \{0\};\par
  212 #if EMULATOR\par
  213     PyRun_SimpleString(\par
  214         "from sense_emu import SenseHat\\n"\par
  215         "sense=SenseHat()\\n"\par
  216         "temp=sense.pressure\\n"\par
  217         "f=open(\\"tempfileforpython.txt\\",\\"w\\")\\n"\par
