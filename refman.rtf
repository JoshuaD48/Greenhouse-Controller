  224     fscanf(fp, "%lf", &reading);\par
  225     fclose(fp);\par
  226     rd.pressure = reading;\par
  227     rd.temperature = 5; //placeholder, use the temperature from the ht221s\par
  228 #else\par
  229     uint8_t temp_out_l = 0, temp_out_h = 0;\par
  230     int16_t temp_out = 0;\par
  231     uint8_t press_out_xl = 0;\par
  232     uint8_t press_out_l = 0;\par
  233     uint8_t press_out_h = 0;\par
  234     int32_t press_out = 0;\par
  235     uint8_t status = 0;\par
  236 \par
  237     // Power down the device (clean start)\par
  238     wiringPiI2CWriteReg8(LPS25Hfd, CTRL_REG1, 0x00);\par
  239 \par
  240     // Turn on the humidity sensor analog front end in single shot mode\par
  241     wiringPiI2CWriteReg8(LPS25Hfd, CTRL_REG1, 0x84);\par
  242 \par
  243     // Run one-shot measurement (temperature and humidity). The set bit will be reset by the\par
  244     // sensor itself after execution (self-clearing bit)\par
  245     wiringPiI2CWriteReg8(LPS25Hfd, CTRL_REG2, 0x01);\par
  246 \par
  247     // Wait until the measurement is completed\par
  248     do\par
  249     \{\par
  250         usleep(HTS221DELAY);    // 25 ms\par
  251         status = wiringPiI2CReadReg8(LPS25Hfd, CTRL_REG2);\par
  252     \}\par
  253     while (status != 0);\par
  254 \par
  255     /* Read the temperature measurement (2 bytes to read) */\par
  256     temp_out_l = wiringPiI2CReadReg8(LPS25Hfd, TEMP_OUT_L);\par
  257     temp_out_h = wiringPiI2CReadReg8(LPS25Hfd, TEMP_OUT_H);\par
  258 \par
  259     /* Read the pressure measurement (3 bytes to read) */\par
  260     press_out_xl = wiringPiI2CReadReg8(LPS25Hfd, PRESS_OUT_XL);\par
  261     press_out_l = wiringPiI2CReadReg8(LPS25Hfd, PRESS_OUT_L);\par
  262     press_out_h = wiringPiI2CReadReg8(LPS25Hfd, PRESS_OUT_H);\par
  263 \par
  264     /* make 16 and 24 bit values (using bit shift) */\par
  265     temp_out = temp_out_h << 8 | temp_out_l;\par
  266     press_out = press_out_h << 16 | press_out_l << 8 | press_out_xl;\par
  267 \par
  268     /* calculate output values */\par
  269     rd.temperature = 42.5 + (temp_out / 480.0);\par
  270     rd.pressure = press_out / 4096.0;\par
  271 \par
  272     // Power down the device\par
  273     wiringPiI2CWriteReg8(LPS25Hfd, CTRL_REG1, 0x00);\par
  274 #endif\par
  275     return rd;\par
  276 \}\par
}
